open Model_app_enums;;

let mod_status_to_string (d)  =
    (match d with
        | MOD_FIX_Status_flying -> "flying"
        | MOD_FIX_Status_crashed -> "crashed"
        | MOD_FIX_Status_reached -> "reached"
    )
;;

let string_to_mod_status_opt (d)  =
    (match d with
        | "flying" -> (Some (MOD_FIX_Status_flying))
        | "crashed" -> (Some (MOD_FIX_Status_crashed))
        | "reached" -> (Some (MOD_FIX_Status_reached))
        | _ -> (None)
    )
;;
