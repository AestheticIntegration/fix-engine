open Full_app_enums;;
open Yojson;;

let status_to_string (d)  =
    (match d with
        | FIX_Status_flying -> "flying"
        | FIX_Status_crashed -> "crashed"
        | FIX_Status_reached -> "reached"
    )
;;

let week_to_string (d)  =
    (match d with
        | FIX_week_noweek -> "noweek"
        | FIX_week_w1 -> "w1"
        | FIX_week_w2 -> "w2"
        | FIX_week_w3 -> "w3"
        | FIX_week_w4 -> "w4"
        | FIX_week_w5 -> "w5"
    )
;;

let status_to_json (d)  : json =
    `String (status_to_string d)
;;

let week_to_json (d)  : json =
    `String (week_to_string d)
;;

let status_opt_to_json (d)  : json =
    (match d with
        | None -> (`Null)
        | Some d -> (status_to_json d)
    )
;;

let week_opt_to_json (d)  : json =
    (match d with
        | None -> (`Null)
        | Some d -> (week_to_json d)
    )
;;
