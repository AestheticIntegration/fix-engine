let check_field_mod_f_NewOrderSingle_Price_inner0 (msg_data : mod_newordersingle_data)  =
    (if (float_GreaterThan_6_6 msg_data.mod_f_NewOrderSingle_Price (float_Create_6 (0))) then
        fields_good_defrej
    else
        (is_fields_invalid_defrej {
            mod_builtin_rejectInfo_text = "Field Price of Message NewOrderSingle violates the constraint: \" it > 0.0\"";
            mod_builtin_rejectInfo_field = Some "Price"
        }))
;;

let check_field_mod_f_NewOrderSingle_Price_outer (msg_data : mod_newordersingle_data)  =
    let check_field_mod_f_NewOrderSingle_Price_inner0_result = check_field_mod_f_NewOrderSingle_Price_inner0 msg_data
    in
    (if check_field_mod_f_NewOrderSingle_Price_inner0_result.field_invalid_defrej then
        check_field_mod_f_NewOrderSingle_Price_inner0_result
    else
        fields_good_defrej)
;;

let check_field_mod_f_NewOrderSingle_Price (msg_data : mod_newordersingle_data) (m_state : model_state)  =
    check_field_mod_f_NewOrderSingle_Price_outer msg_data
;;

let check_fields_NewOrderSingle (msg_data : mod_newordersingle_data) (m_state : model_state)  =
    let mod_f_NewOrderSingle_Price_result = check_field_mod_f_NewOrderSingle_Price msg_data m_state
    in
    (if mod_f_NewOrderSingle_Price_result.field_invalid_defrej then
        mod_f_NewOrderSingle_Price_result
    else
        fields_good_defrej)
;;

