open Full_app_records;;
open Parse_base_types;;
open Parse_datetime;;
open Parser_utils.Parser;;

let parse_interval (msg)  =
    opt msg "" parse_Duration (fun msg f_interval_duration -> opt msg "" parse_UTCTimestamp_milli (fun msg f_interval_start_time -> (ParseSuccess {
        f_interval_start_time = f_interval_start_time;
        f_interval_duration = f_interval_duration
    },msg)
    )
    )
;;

let parse_Position (msg)  =
    opt msg "" parse_int (fun msg f_Position_y -> opt msg "" parse_int (fun msg f_Position_x -> (ParseSuccess {
        f_Position_x = f_Position_x;
        f_Position_y = f_Position_y
    },msg)
    )
    )
;;
