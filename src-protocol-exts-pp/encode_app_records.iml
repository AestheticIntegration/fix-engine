open Encode_base_types;;
open Encode_datetime;;
open Encode_utils;;
open Full_app_records;;

let encode_interval (msg)  =
    (@) (("",enc_opt encode_UTCTimestamp_milli msg.f_interval_start_time) :: (("",enc_opt encode_Duration msg.f_interval_duration) :: [])) []
;;

let encode_Position (msg)  =
    (@) (("",enc_opt encode_int msg.f_Position_x) :: (("",enc_opt encode_int msg.f_Position_y) :: [])) []
;;
