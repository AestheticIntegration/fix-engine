let mod_enum_side_decoder  : model_fix_side decoder =
    string >>= (fun x -> (match string_to_mod_side_opt x with
        | None -> (fail "Unknown enum (Side) encoding.")
        | Some y -> (succeed y)
    )
    )
    [@@macro]
;;

