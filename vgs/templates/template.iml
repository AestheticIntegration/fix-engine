type t =
    | Template_Move 
    | Template_SetState 
;;

let data (x : t)  =
    (match x with
        | Template_Move -> "Actions.m_action_fix_move"
        | Template_SetState -> "Actions.m_action_fix_setstate"
    )
;;

let concrete (x : t)  =
    (match x with
        | Template_Move -> ("Scenario.Action" :: ("Actions.FIX_Action_Move" :: []))
        | Template_SetState -> ("Scenario.Action" :: ("Actions.FIX_Action_SetState" :: []))
    )
;;
