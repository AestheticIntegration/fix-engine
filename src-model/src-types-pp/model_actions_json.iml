[@@@import "../src-types/actions.iml"]
[@@@import "../../src-core-pp/base_types_json.iml"]
[@@@import "../../src-core-pp/datetime_json.iml"]
[@@@import "../../src-protocol-exts-pp/json_generator_utils.iml"]
[@@@program]
open Actions;;
open Base_types_json;;
open Datetime_json;;
open Json_generator_utils;;
open Yojson;;

let mod_act_x_to_json (x)  : json =
    assoc_filter_nulls (("x",int_to_json x.mod_f_x_x) :: [])
;;

let json_of_fix_action (x)  : json =
    (match x with
        | FIX_Action_x x -> (`Assoc (("tag",`String "x") :: (("data",mod_act_x_to_json x) :: [])))
    )
;;
[@@@logic]
