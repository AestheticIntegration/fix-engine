let mod_act_x_opt_to_json (y)  : json =
    (match y with
        | None -> `Null
        | Some x -> (assoc_filter_nulls ([(("x",int_to_json x.mod_f_x_x))]))
    )
;;

let mod_act_x_to_json (x)  : json =
    assoc_filter_nulls ([(("x",int_to_json x.mod_f_x_x))])
;;

