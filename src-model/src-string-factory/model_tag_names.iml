open Model_tags;;

let tag_to_message (x)  =
    (match x with
        | "D" -> (Some M_Msg_NewOrderSingle_Tag)
        | "F" -> (Some M_Msg_OrderCancel_Tag)
        | "AB" -> (Some M_Msg_NewOrder_Multi_Leg_Tag)
        | "E" -> (Some M_Msg_NewOrder_List_Tag)
        | "K" -> (Some M_Msg_ListOrderCancel_Tag)
        | "j" -> (Some M_Msg_BusinessMessageReject_Tag)
        | "AS" -> (Some M_Msg_AllocationReport_Tag)
        | "AT" -> (Some M_Msg_AllocationReportACK_Tag)
        | "J" -> (Some M_Msg_AllocationInstruction_Tag)
        | "0" -> (Some M_Msg_Heartbeat_Tag)
        | "1" -> (Some M_Msg_TestRequest_Tag)
        | "2" -> (Some M_Msg_ResendRequest_Tag)
        | "3" -> (Some M_Msg_SessionLevelReject_Tag)
        | "4" -> (Some M_Msg_SequenceResetRequest_Tag)
        | "5" -> (Some M_Msg_Logout_Tag)
        | "A" -> (Some M_Msg_FIXLogon_Tag)
        | "Q" -> (Some M_Msg_DontKnowTrade_Tag)
        | _ -> None
    )
;;

