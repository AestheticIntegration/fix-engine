(rule
  (targets full_app_enums.ml)
  (deps full_app_enums.iml)
   (action
    (with-stdout-to full_app_enums.ml
       (run imandra-extract full_app_enums.iml))))

(rule
  (targets full_app_messages.ml)
  (deps full_app_messages.iml)
   (action
    (with-stdout-to full_app_messages.ml
       (run imandra-extract full_app_messages.iml))))

(rule
  (targets full_app_records.ml)
  (deps full_app_records.iml)
   (action
    (with-stdout-to full_app_records.ml
       (run imandra-extract full_app_records.iml))))

(rule
  (targets full_app_tags.ml)
  (deps full_app_tags.iml)
   (action
    (with-stdout-to full_app_tags.ml
       (run imandra-extract full_app_tags.iml))))

(library
  (name protocol_exts)
  (public_name protocol_exts)
  (wrapped false)
  (flags (:standard -open Imandra_prelude))
  (libraries imandra-prelude core core_time_defaults)
)
