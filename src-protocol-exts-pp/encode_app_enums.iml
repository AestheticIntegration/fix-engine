open Full_app_enums;;

let encode_Status (d)  =
    (match d with
        | FIX_Status_flying -> "flying"
        | FIX_Status_crashed -> "crashed"
        | FIX_Status_reached -> "reached"
    )
;;

let encode_week (d)  =
    (match d with
        | FIX_week_noweek -> "noweek"
        | FIX_week_w1 -> "w1"
        | FIX_week_w2 -> "w2"
        | FIX_week_w3 -> "w3"
        | FIX_week_w4 -> "w4"
        | FIX_week_w5 -> "w5"
    )
;;
